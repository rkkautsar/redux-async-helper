{
  "name": "redux-companion",
  "version": "0.6.3",
  "description": "A tool to help reducing redux code boilerplate",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf dist/",
    "build": "rollup -c",
    "example": "parcel example/index.html",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rkkautsar/redux-companion.git"
  },
  "keywords": [
    "redux",
    "async",
    "react",
    "utils",
    "es6"
  ],
  "author": "Rakha Kanz Kautsar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rkkautsar/redux-companion/issues"
  },
  "homepage": "https://github.com/rkkautsar/redux-companion#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime": "^7.1.2",
    "@types/seamless-immutable": "^7.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "jest": "^24.1.0",
    "parcel-bundler": "^1.10.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^6.0.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "release-it": "^10.1.0",
    "rimraf": "^2.6.2",
    "rollup": "^1.2.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.4"
  },
  "dependencies": {},
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/*.js",
      "!**/node_modules/**"
    ],
    "setupFiles": [
      "<rootDir>/node_modules/regenerator-runtime/runtime"
    ]
  }
}
