{
  "name": "redux-companion",
  "version": "0.8.0",
  "description": "A tool to help reducing redux code boilerplate",
  "scripts": {
    "test": "jest"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rkkautsar/redux-companion.git"
  },
  "keywords": [
    "redux",
    "async",
    "react",
    "utils",
    "es6"
  ],
  "author": "Rakha Kanz nKautsar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rkkautsar/redux-companion/issues"
  },
  "homepage": "https://github.com/rkkautsar/redux-companion#readme",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "babel-jest": "^24.1.0",
    "husky": "^3.1.0",
    "jest": "^24.1.0",
    "lerna": "^3.18.4",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "packages/**/lib/*.js",
      "!**/node_modules/**"
    ]
  },
  "workspaces": [
    "packages/*",
    "example"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
